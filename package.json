{
  "name": "bee-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx serve@latest out",
    "build:start": "npm run build && npm run start",
    "lint:build:start": "npm run lint:fix && npm run build && npm run start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean:imports": "eslint **/*.{ts,tsx} --fix --quiet",
    "clean:deps": "depcheck",
    "clean:all": "npm run clean:imports && npm run clean:deps",
    "pwa:icons": "node scripts/generate-pwa-icons.js",
    "pwa:audit": "npx lighthouse http://localhost:3000 --preset=desktop --chrome-flags='--headless' --output=html --output-path=./lighthouse-report.html",
    "pwa:check": "npm run build && npm run start & sleep 5 && npm run pwa:audit && pkill -f serve"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@vercel/speed-insights": "^1.2.0",
    "aos": "^3.0.0-beta.6",
    "country-flag-icons": "^1.5.19",
    "cssnano": "^7.0.7",
    "next": "15.3.3",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/aos": "^3.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "^8",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
